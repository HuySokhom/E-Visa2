@model eVisa.ViewModel.HomeView
@{
    ViewBag.Title = "Kingdom Of Cambodia Ministry Of Foreign Affairs & International Cooperation";
    var i = 0;
    var j = 0;

    eVisa.Function.BaseFunction fun = new eVisa.Function.BaseFunction();
    Dictionary<int, string> Menus = (Dictionary<int, string>)ViewBag.MenuList;
}
<link href="/Content/slider.css" rel="stylesheet">
<div id="jssor_1"
     style="
        position: relative;
        margin: 0 auto;
        top: 0px; left: 0px;
        width: 1300px; height: 500px;
        overflow: hidden; visibility: hidden;
    ">
    <!-- Loading Screen -->
    <div data-u="loading" style="position: absolute; top: 0px; left: 0px;">
        <div 
             style="
                filter: alpha(opacity=70); 
                opacity: 0.7;
                position: absolute; 
                display: block; 
                top: 0px; left: 0px;
                width: 100%; 
                height: 100%;"
        >
        </div>
        <div 
             style="
                position:absolute;
                display:block;
                background:url('Images/loading.gif') no-repeat center center;
                top:0px;
                left:0px;
                width:100%;
                height:100%;
            ">
        </div>
    </div>
    <div data-u="slides"
        style="
            cursor: default; position: relative;
            top: 0px; left: 0px; width: 1300px;
            height: 500px; overflow: hidden;
        "
    >
        @foreach (var item in Model.ImageView)
        {
             <div data-p="225.00" style="display: none;">
                 <img data-u="image" src="@item.Url" />
            </div>
        }
    </div>
    <!-- Bullet Navigator -->
    <div data-u="navigator" class="jssorb05" style="bottom:16px;right:16px;" data-autocenter="1">
        <!-- bullet navigator item prototype -->
        <div data-u="prototype" style="width:16px;height:16px;"></div>
    </div>
    <!-- Arrow Navigator -->
    <span data-u="arrowleft" class="jssora22l" style="top:0px;left:12px;width:40px;height:58px;" data-autocenter="2"></span>
    <span data-u="arrowright" class="jssora22r" style="top:0px;right:12px;width:40px;height:58px;" data-autocenter="2"></span>
</div>

<div class="row" style="margin-bottom: 0px;">
    <div class="content">
        <div class="container">
            <div class="col-md-8">
                <div class="" style="margin-top: 15px;">
                    @if (@Model.PageAnnounment != null)
                    {
                        <div class="announcement-heading">
                            <h1>@Model.PageAnnounment.Title</h1>
                            @Html.Raw(Model.PageAnnounment.Content)
                        </div>
                    }

                    @if (@Model.Pageview != null)
                    {
                        <h1>@Model.Pageview.Title</h1>
                        @Html.Raw(Model.Pageview.Content)
                    }

                    <h1>Tourist visa</h1>
                    <table class="table table-bordered">
                        @foreach (var item in Model.TouristTable)
                        {
                            <tr style="@(item.Header ? "font-weight: bold;" : "")">
                                <td style="vertical-align: top; width: 200px;"> @item.Name </td>
                                <td style="vertical-align:top;"> @item.Text </td>
                            </tr>
                        }
                    </table>

                    <h1>Name of Port(View Map)</h1>
                    <table class="table table-bordered">
                        <tbody>
                            @foreach (var item in Model.NameofPort)
                            {
                                <tr style="@(item.Header ? "font-weight: bold;" : "")">
                                    <td width="75%"> @item.Name </td>
                                    <td width="10%"> @item.Border </td>
                                    <td width="10%"> @item.Entry </td>
                                    <td width="20%"> @item.Exit </td>
                                </tr>
                            }

                        </tbody>
                    </table>

                </div>
            </div>
            
            <div class="col-md-4" style="margin-top: 8px; border: 1px solid #ddd; margin-bottom: 15px;">

                <img src="/Images/Logo/e-visa-home-iphone.png" style="padding: 9px; width: 100px; margin-bottom: -10px;" />
                <div class="">
                    <div class="support ">
                        <div class="support-item ">
                            <div class="support-item ">
                                <a href="/Apply">
                                    <span class="fa fa-check-square-o"></span>
                                    @Menus[21]
                                </a>
                            </div>
                            <a href="/CheckStatus">
                                <span class="fa fa-download"></span>
                                @Menus[20]
                            </a>
                        </div>
                        <div class="support-item ">
                            <a href="/Home/SearchReferenceNumber">
                                <span class="fa fa-search"></span>
                                @Menus[24]
                            </a>
                        </div>
                        <div class="support-item">
                            <a href="/Information/HowToApply" style="margin-left: 4px;">
                                <span class="fa fa-question"></span>
                                @Menus[25]
                            </a>
                        </div>
                        <div class="support-item ">
                            <a href="/User/FQLogin">
                                <span class="fa fa-user"></span>
                                @Menus[22]
                            </a>
                        </div>
                    </div>
                </div>
                
                <ul class="collapsible" data-collapsible="accordion">
                    <li>
                        <div class="collapsible-header">
                            <i class="accordion_icon fa fa-plus"></i>
                            Online Support (24/7)
                        </div>
                        <div class="collapsible-body">
                            <div class="support ">
                                <div class="support-item ">
                                    <a href="#"> <span class="fa fa-skype fa-lg fa-fw">  </span>  Skype </a>
                                </div>
                                <div class="support-item">
                                    <a href="#">
                                        <span class="fa fa-dollar fa-lg fa-fw">  </span>  Payment Support Chat
                                    </a>
                                </div>
                                <div class="support-item ">
                                    <a href="#">
                                        <span class="fa fa-photo fa-lg fa-fw">  </span> Photo Support Chat
                                    </a>
                                </div>
                                <div class="support-item ">
                                    <a href="#">
                                        <span class="fa fa-wrench fa-lg fa-fw">  </span> Tech Support Chat
                                    </a>
                                </div>
                                <div class="support-item ">
                                    <a href="#">
                                        <span class="fa fa-info-circle fa-lg fa-fw">  </span> General Info Support Chat
                                    </a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="collapsible-header">
                            <i class="accordion_icon fa fa-plus"></i>
                            Map
                        </div>
                        <div class="collapsible-body">
                            <div class="support ">
                                <a href="/Information/Map">
                                    <img src="/Images/Images/Home/map.png" style=" display: inline; border: 1px solid #ddd;" class="img-responsive" />
                                </a>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="collapsible-header">
                            <i class="accordion_icon fa fa-plus"></i>
                            Photo Guidelines
                        </div>
                        <div class="collapsible-body">
                            <div class="support ">
                                <a href="/Information/Guidelines">
                                    <img src="/Images/Images/Home/guide.png" style="display: inline; border: 1px solid #ddd;" class="img-responsive" />
                                </a>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="collapsible-header">
                            <i class="accordion_icon fa fa-plus"></i>
                            Cambodia Photo Galleries
                        </div>
                        <div class="collapsible-body">
                            <div class="support ">
                                <embed type="application/x-shockwave-flash"
                                       src="https://static.googleusercontent.com/external_content/picasaweb.googleusercontent.com/slideshow.swf"
                                       width="99.50%"
                                       height="300px"
                                       flashvars="host=picasaweb.google.com&hl=en_US&feat=flashalbum&RGB=0x000000&feed=https%3A%2F%2Fpicasaweb.google.com%2Fdata%2Ffeed%2Fapi%2Fuser%2F107288189848824308171%3Falt%3Drss%26kind%3Dphoto%26access%3Dpublic%26psc%3DF%26q%26uname%3D107288189848824308171"
                                       pluginspage="http://www.macromedia.com/go/getflashplayer" />
                                <span style="color: rgb(153, 153, 153); font-size: 12px; z-index:1;">
                                    <a href="https://picasaweb.google.com/107288189848824308171" target="_blank">
                                        <i>(Click here to see more)</i>
                                    </a>
                                </span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="collapsible-header">
                            <i class="accordion_icon fa fa-plus"></i>
                            Information
                        </div>
                        <div class="collapsible-body">
                            <div class="support ">
                                @if (@Model.PageInformation != null)
                                {
                                    @Html.Raw(Model.PageInformation.Content)
                                }
                            </div>
                        </div>
                    </li>
                </ul>
            </div>


        </div>
    </div>
</div>
@section Scripts
{
<script type="text/javascript" src="/Scripts/jssor.slider.min.js"></script>
<script type="text/javascript" src="/Scripts/slider.js"></script>
<script type="text/javascript">
    jQuery('.collapsible li').click(function () {
        jQuery(this).toggleClass('active').find('i').toggleClass('fa-plus fa-minus')
               .closest('li').siblings('li')
               .removeClass('active').find('i').removeClass('fa-minus').addClass('fa-plus');
        jQuery(this).next('.collapsible-body').slideToggle().siblings('.collapsible-body').slideUp();

    });
</script>


    
 }

